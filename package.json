{
  "name": "next-component-factory-discord-bot",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build:next": "next build",
    "start:next": "next start --port ${PORT-3000}",
    "build:bot": "rm -rf dist && tsc -p ./tsconfig.bot.json",
    "start:bot": "node dist/discord-bot/startup.js",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "build": "npm run build:bot && npm run build:next",
    "start": "npm run start:bot && npm run start:next",
    "prisma": "npx prisma generate && npx prisma db push",
    "type-check": "tsc --noEmit",
    "dev:next": "next dev",
    "dev:bot": "nodemon"
  },
  "dependencies": {
    "@discordjs/rest": "1.5.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@hookform/resolvers": "^2.9.10",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@next/font": "^13.1.2",
    "@notionhq/client": "^2.1.1",
    "@prisma/client": "4.7.1",
    "@react-slip-and-slide/web": "^1.15.0",
    "@react-spring/web": "^9.6.1",
    "@tanstack/react-query": "^4.10.0",
    "@trpc/client": "10.0.0-rc.4",
    "@trpc/next": "10.0.0-rc.4",
    "@trpc/react-query": "10.0.0-rc.4",
    "@trpc/server": "10.0.0-rc.4",
    "@tryfabric/martian": "^1.2.4",
    "@types/dedent": "^0.7.0",
    "@uiw/react-md-editor": "^3.20.2",
    "aws-sdk": "^2.1295.0",
    "clsx": "^1.2.1",
    "dayjs": "^1.11.7",
    "dedent": "^0.7.0",
    "discord.js": "14.7.1",
    "dotenv": "^16.0.1",
    "framer-motion": "^6.5.1",
    "immer": "^9.0.17",
    "lodash": "^4.17.21",
    "next": "13.0.2",
    "next-auth": "^4.15.1",
    "next-remove-imports": "^1.0.8",
    "node-cron": "^3.0.2",
    "react": "18.2.0",
    "react-chartjs-2": "^5.1.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.42.1",
    "react-switch": "^7.0.0",
    "react-toggle-dark-mode": "^1.1.1",
    "slugify": "^1.6.5",
    "styled-components": "^5.3.6",
    "superjson": "1.9.1",
    "tailwind-merge": "^1.8.1",
    "terser": "^5.16.1",
    "uuid": "^9.0.0",
    "uuidv4": "^6.2.13",
    "winston": "^3.8.2",
    "zod": "^3.18.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.0.0",
    "@types/node-cron": "^3.0.7",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "autoprefixer": "^10.4.13",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "^8.26.0",
    "eslint-config-next": "13.0.2",
    "eslint-plugin-tailwindcss": "^3.8.0",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.21",
    "prettier": "^2.8.3",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "prisma": "4.7.1",
    "tailwind-styled-components": "^2.2.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "4.8.4"
  },
  "ct3aMetadata": {
    "initVersion": "6.10.1"
  },
  "madge": {
    "fontSize": "16px",
    "fileExtensions": [
      "ts"
    ],
    "backgroundColor": "#ffffff",
    "noDependencyColor": "#249bc7",
    "nodeColor": "#c78024",
    "cyclicNodeColor": "#ff0000",
    "edgeColor": "#7b24c7",
    "detectiveOptions": {
      "ts": {
        "mixedImports": true,
        "skipTypeImports": true
      }
    },
    "graphVizOptions": {
      "G": {
        "rankdir": "LR"
      }
    }
  }
